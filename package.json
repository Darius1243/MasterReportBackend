{
	"name": "master-report-backend",
	"version": "1.0.0",
	"description": "master-report-backend",
	"main": "dist/index.js",
	"scripts": {
		"start": "node dist/index.js",
		"dev": "nodemon",
		"build": "tsc && copyfiles -u 2 \"src/generated/**/*\" dist/generated",
		"prisma:migrate:dev": "prisma migrate dev",
		"prisma:generate": "prisma generate",
		"prisma:studio": "prisma studio",
		"prisma:deploy": "prisma migrate deploy",
		"dev:reload": "yarn prisma:migrate:dev --name autogen_migration && yarn prisma:generate && yarn build && yarn start"
	},
	"keywords": [
		"express",
		"graphql",
		"typescript",
		"sqlite",
		"module-alias"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@apollo/server": "^4.12.2",
		"@as-integrations/express5": "^1.1.2",
		"apollo-server-express": "^3.13.0",
		"body-parser": "^2.2.0",
		"class-validator": "^0.14.2",
		"cors": "^2.8.5",
		"express": "5.1.0",
		"generate": "^0.14.0",
		"graphql": "^16.11.0",
		"graphql-fields": "^2.0.3",
		"graphql-scalars": "^1.24.2",
		"module-alias": "^2.2.3",
		"npx": "^10.2.2",
		"reflect-metadata": "^0.2.2",
		"sqlite": "^5.1.1",
		"sqlite3": "^5.1.7",
		"type-graphql": "^2.0.0-rc.2",
		"typegraphql-prisma": "^0.28.0"
	},
	"devDependencies": {
		"@prisma/client": "5.18.0",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/graphql-fields": "^1.3.9",
		"@types/module-alias": "^2.0.4",
		"@types/node": "24.3.0",
		"copyfiles": "^2.4.1",
		"nodemon": "^3.1.10",
		"prisma": "5.18.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.2"
	},
	"_moduleAliases": {
		"@generated": "dist/generated",
		"@services": "dist/services",
		"@features": "dist/features",
		"@config": "dist/config",
		"@utils": "dist/utils"
	}
}
